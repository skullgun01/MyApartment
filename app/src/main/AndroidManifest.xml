<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="th.co.appman.myapartment">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApartment">
        <activity android:name=".view.user.exit.ExitRoomActivity"></activity>
        <activity android:name=".view.user.payment.MyPaymentActivity" />
        <activity android:name=".view.user.room.MyRoomActivity" />
        <activity android:name=".view.user.menu.UserMenuActivity" />
        <activity android:name=".view.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>